{
	"name": "@vscode/wasi",
	"description": "WASI preview 2 interfaces for VS Code",
	"main": "./lib/main.js",
	"typings": "./lib/main.d.ts",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"all": "npm run clean && npm run compile && npm run lint",
		"all:publish": "git clean -xfd . && npm install && npm run compile:publish && npm run lint",
		"clean": "node ../node_modules/rimraf/bin.js lib",
		"compile": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.json",
		"compile:publish": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.publish.json",
		"generate": "node ../wasm-component-model/lib/tools/main.js --outDir ./src --resourceStyle module --package wasi ./src/wasi.json",
		"prepack": "npm run all:publish",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"dependencies": {},
	"devDependencies": {
		"@playform/build": "0.0.7"
	},
	"enabledApiProposals": []
}
