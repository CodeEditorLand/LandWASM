{
	"name": "@vscode/wasm-wasi",
	"version": "0.0.1",
	"private": false,
	"description": "An API facade for the WASM WASI CORE VS Code extension",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandWASM#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandWASM/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandWASM.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./lib/api/main.js",
	"typings": "./lib/api/main.d.ts",
	"bin": {
		"dir-dump": "./bin/dir-dump"
	},
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"all": "npm run clean && npm run compile && npm run lint",
		"all:publish": "git clean -xfd . && npm install && npm run update:version && npm run compile:publish && npm run lint",
		"clean": "node ../node_modules/rimraf/bin.js lib",
		"compile": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.json",
		"compile:publish": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.publish.json",
		"prepack": "npm run all:publish",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"update:version": "node ./bin/updateVersion.mjs"
	},
	"dependencies": {
		"semver": "7.6.0",
		"yargs": "17.7.2"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/semver": "7.5.8",
		"@types/yargs": "17.0.32"
	},
	"publishConfig": {
		"access": "public"
	},
	"enabledApiProposals": []
}
