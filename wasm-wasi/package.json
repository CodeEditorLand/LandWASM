{
  "name": "@vscode/wasm-wasi",
  "version": "0.0.1",
  "description": "An API facade for the WASM WASI CORE VS Code extension",
  "engines": {
    "node": ">=16.17.1",
    "vscode": "1.78.0"
  },
  "main": "./lib/api/main.js",
  "typings": "./lib/api/main.d.ts",
  "bin": {
    "dir-dump": "./bin/dir-dump"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "keywords": [
    "VSCode",
    "API"
  ],
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "enabledApiProposals": [],
  "dependencies": {
    "yargs": "17.7.2",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@types/vscode": "1.83.1",
    "@types/yargs": "17.0.29",
    "@types/semver": "7.5.4",
    "typescript-esbuild": "0.3.1"
  },
  "scripts": {
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts",
    "prepack": "npm run all:publish",
    "compile": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.json",
    "compile:publish": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.publish.json",
    "watch": "node ../node_modules/typescript/lib/tsc.js -b ./tsconfig.watch.json -w",
    "clean": "node ../node_modules/rimraf/bin.js lib",
    "lint": "node ../node_modules/eslint/bin/eslint.js --ext ts src",
    "all": "npm run clean && npm run compile && npm run lint",
    "all:publish": "git clean -xfd . && npm install && npm run update:version && npm run compile:publish && npm run lint",
    "update:version": "node ./bin/updateVersion.mjs"
  },
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  }
}
