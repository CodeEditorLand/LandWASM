{
  "name": "wasm-wasi-c-example",
  "private": "false",
  "publisher": "vscode-samples",
  "version": "0.0.1",
  "description": "C Example",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "license": "MIT",
  "engines": {
    "vscode": "1.77.0"
  },
  "main": "./dist/desktop/extension.js",
  "browser": "./dist/web/extension.js",
  "activationEvents": [],
  "extensionDependencies": [
    "ms-vscode.wasm-wasi-core"
  ],
  "contributes": {
    "commands": [
      {
        "command": "wasm-wasi-c-example.run",
        "category": "WASM Example",
        "title": "Run C Hello World"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "1.83.1",
    "typescript-esbuild": "0.3.1"
  },
  "dependencies": {
    "@vscode/wasm-wasi": "0.11.0-next.0"
  },
  "scripts": {
    "build": "npm run compile:ts && npm run build:wasi && npm run esbuild",
    "compile:ts": "tsc -p tsconfig.json",
    "build:wasi": "clang hello.c -o ./hello.wasm",
    "esbuild": "node ./bin/esbuild.js",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  }
}
