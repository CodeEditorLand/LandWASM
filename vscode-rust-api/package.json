{
	"private": "true",
	"name": "@vscode/rust-api",
	"version": "0.0.1",
	"description": "Rust VS Code API bindings",
	"author": "Visual Studio Code Team",
	"license": "MIT",
	"keywords": [
		"VSCode",
		"Rust"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/microsoft/vscode-wasm.git"
	},
	"bugs": {
		"url": "https://github.com/microsoft/vscode-wasm/issues"
	},
	"homepage": "https://github.com/microsoft/vscode-wasm#readme",
	"engines": {
		"vscode": "^1.87.0",
		"node": ">=18.18.2"
	},
	"exports": {
		".": {
			"types": "./lib/main.d.ts",
			"node": "./lib/main.js",
			"browser": "./lib/main.js"
		}
	},
	"devDependencies": {
		"@types/vscode": "1.87.0"
	},
	"scripts": {
		"build": "npm run compile:ts && npm run build:rust",
		"compile:ts": "node ../node_modules/typescript/lib/tsc.js -b tsconfig.json",
		"watch:ts": "node ../node_modules/typescript/lib/tsc.js -b tsconfig.json -w",
		"build:rust": "cargo build --target wasm32-unknown-unknown",
		"generate:model": "node ../wasm-component-model/lib/tools/main.js --noMain --outDir ./src/ ./rust/wit",
		"generate:wat": "wasm-tools print ./target/wasm32-unknown-unknown/debug/vscode.wasm > ./target/wasm32-unknown-unknown/debug/vscode.wat",
		"run": "npm run build:rust && npm run compile:ts && node ./out/main.js"
	}
}