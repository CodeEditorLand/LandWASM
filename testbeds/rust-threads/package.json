{
	"name": "testbed-rust-threads",
	"version": "0.0.1",
	"private": false,
	"description": "Rust Threads Example",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandWASM#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandWASM/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandWASM.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build": "npm run compile:ts && npm run build:rust",
		"build:rust": "cargo build --target wasm32-wasi-preview1-threads",
		"compile:ts": "node ../../node_modules/typescript/lib/tsc.js -b tsconfig.json",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"wasmtime": "wasmtime --wasm-features=threads --wasi-modules=experimental-wasi-threads target/wasm32-wasi-preview1-threads/debug/rust-threads.wasm",
		"watch:ts": "node ../../node_modules/typescript/lib/tsc.js -b tsconfig.json -w"
	},
	"contributes": {
		"commands": [
			{
				"category": "wasm",
				"command": "testbed-rust-threads.run",
				"title": "Run Rust Thread Program"
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6"
	},
	"publishConfig": {
		"access": "public"
	}
}
