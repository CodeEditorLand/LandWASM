{
	"compilerOptions": {
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strict": true,
		"module": "Node16",
		"moduleResolution": "Node16",
		"target": "es2020",
		"outDir": "out",
		"rootDir": "src",
		"lib": ["es2020", "WebWorker"],
		"sourceMap": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
