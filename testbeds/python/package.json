{
  "name": "testbed-python",
  "private": "false",
  "publisher": "ms-vscode",
  "version": "0.0.1",
  "description": "Python Example",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "license": "MIT",
  "engines": {
    "vscode": "1.67.0"
  },
  "main": "./out/extension.js",
  "browser": "./dist/web/extension.js",
  "activationEvents": [
    "onCommand:testbed-python.webshell.python"
  ],
  "extensionDependencies": [
    "ms-vscode.wasm-wasi-core",
    "ms-vscode.webshell"
  ],
  "contributes": {
    "commands": [
      {
        "command": "testbed-python.runFile",
        "category": "wasm",
        "title": "Run Python File",
        "enablement": "editorLangId == python"
      },
      {
        "command": "testbed-python.runInteractive",
        "category": "wasm",
        "title": "Run Interactive Python Shell"
      }
    ],
    "menus": {
      "editor/title/run": [
        {
          "command": "testbed-python.runFile",
          "when": "resourceLangId == python",
          "group": "navigation@1"
        },
        {
          "command": "testbed-python.runInteractive",
          "when": "resourceLangId == python",
          "group": "navigation@2"
        }
      ]
    },
    "webShellMountPoints": [
      {
        "mountPoint": "/usr/local/lib/python3.12",
        "path": "wasm/lib"
      },
      {
        "mountPoint": "/usr/bin/python",
        "command": "testbed-python.webshell.python"
      }
    ]
  },
  "scripts": {
    "build": "npm run compile",
    "compile": "node ../../node_modules/typescript/lib/tsc.js -b tsconfig.json",
    "watch": "node ../../node_modules/typescript/lib/tsc.js -b tsconfig.json -w",
    "esbuild": "node ./bin/esbuild.js",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript-esbuild": "0.3.1"
  }
}
