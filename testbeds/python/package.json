{
	"name": "testbed-python",
	"version": "0.0.1",
	"private": false,
	"description": "Python Example",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"browser": "./dist/web/extension.js",
	"scripts": {
		"build": "npm run compile",
		"compile": "node ../../node_modules/typescript/lib/tsc.js -b tsconfig.json",
		"esbuild": "node ./bin/esbuild.js",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"watch": "node ../../node_modules/typescript/lib/tsc.js -b tsconfig.json -w"
	},
	"contributes": {
		"commands": [
			{
				"category": "wasm",
				"command": "testbed-python.runFile",
				"enablement": "editorLangId == python",
				"title": "Run Python File"
			},
			{
				"category": "wasm",
				"command": "testbed-python.runInteractive",
				"title": "Run Interactive Python Shell"
			}
		],
		"menus": {
			"editor/title/run": [
				{
					"command": "testbed-python.runFile",
					"group": "navigation@1",
					"when": "resourceLangId == python"
				},
				{
					"command": "testbed-python.runInteractive",
					"group": "navigation@2",
					"when": "resourceLangId == python"
				}
			]
		},
		"webShellMountPoints": [
			{
				"mountPoint": "/usr/local/lib/python3.12",
				"path": "wasm/lib"
			},
			{
				"command": "testbed-python.webshell.python",
				"mountPoint": "/usr/bin/python"
			}
		]
	},
	"activationEvents": [
		"onCommand:testbed-python.webshell.python"
	],
	"devDependencies": {
		"typescript-esbuild": "latest"
	},
	"extensionDependencies": [
		"ms-vscode.wasm-wasi-core",
		"ms-vscode.webshell"
	],
	"publishConfig": {
		"access": "public"
	}
}
