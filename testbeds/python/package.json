{
	"activationEvents": [
		"onCommand:testbed-python.webshell.python"
	],
	"author": {
		"email": "Land@PlayForm.Cloud",
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"browser": "./dist/web/extension.js",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandWASM/issues"
	},
	"contributes": {
		"commands": [
			{
				"category": "wasm",
				"command": "testbed-python.runFile",
				"enablement": "editorLangId == python",
				"title": "Run Python File"
			},
			{
				"category": "wasm",
				"command": "testbed-python.runInteractive",
				"title": "Run Interactive Python Shell"
			}
		],
		"menus": {
			"editor/title/run": [
				{
					"command": "testbed-python.runFile",
					"group": "navigation@1",
					"when": "resourceLangId == python"
				},
				{
					"command": "testbed-python.runInteractive",
					"group": "navigation@2",
					"when": "resourceLangId == python"
				}
			]
		},
		"webShellMountPoints": [
			{
				"mountPoint": "/usr/local/lib/python3.12",
				"path": "wasm/lib"
			},
			{
				"command": "testbed-python.webshell.python",
				"mountPoint": "/usr/bin/python"
			}
		]
	},
	"description": "Python Example",
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6"
	},
	"extensionDependencies": [
		"ms-vscode.wasm-wasi-core",
		"ms-vscode.webshell"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandWASM#readme",
	"keywords": [
		"land"
	],
	"license": "SEE LICENSE IN LICENSE",
	"main": "./out/extension.js",
	"name": "testbed-python",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandWASM.git"
	},
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build": "npm run compile",
		"compile": "node ../../node_modules/typescript/lib/tsc.js -b tsconfig.json",
		"esbuild": "node ./bin/esbuild.js",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"type": "module",
	"version": "0.0.1"
}
