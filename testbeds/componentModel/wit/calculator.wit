// wit/host.wit
package vscode:example;

interface types {
	record operands {
		left: u32,
		right: u32
	}

	variant operation {
		add(operands),
		sub(operands),
		mul(operands),
		div(operands)
	}
}

world calculator {
	use types.{ operation };
	export add: func(a: u32, b: u32) -> u32;
	export calc: func(o: operation) -> u32;
}